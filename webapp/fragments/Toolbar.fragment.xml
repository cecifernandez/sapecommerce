<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <Toolbar
        width="95%"
        class="toolbar"
    >
        <Button
            text="Home"
            press="onNavToHome"
            class="btn"
        />
        <Button
            id="productsText"
            text="Products"
            press="onNavToProducts"
            class="btn"
        />

        <Button
            text="Account"
            press="onNavToAccount"
            class="btn"
        />
        <Button
            text="Cart"
            press="openCart"
            class="btn"
        />
    </Toolbar>
</core:FragmentDefinition>
