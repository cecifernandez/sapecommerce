{
  "_version": "1.65.0",
  "sap.app": {
    "id": "ui5.starwarsecommerce",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.17.1",
      "toolsId": "0e2975b6-ecd6-4d17-9c49-6a92eeeb2590"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.134.1",
      "libs": {
        "sap.m": {},
        "sap.f": {},
        "sap.ui.core": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ui5.starwarsecommerce.i18n.i18n"
        }
      },
      "productModel": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "./model/Products.json",
        "settings": {
          "bindRequest": true
        }
      },
      "userModel": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/userModel.json",
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "rootView": {
      "viewName": "ui5.starwarsecommerce.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "viewType": "XML",
        "viewPath": "ui5.starwarsecommerce.view",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "home",
          "target": "home"
        },
        {
          "pattern": "products",
          "name": "productsList",
          "target": "products"
        },
        {
          "pattern": "products/{category}",
          "name": "productsByCategory",
          "target": "products"
        },
        {
          "pattern": "products/{category}/{subcategory}",
          "name": "productsBySubcategory",
          "target": "products"
        },
        {
          "pattern": "product/{productId}",
          "name": "productDetail",
          "target": "productDetail"
        },
        {
          "pattern": "account",
          "name": "account",
          "target": "account"
        }
      ],
      "targets": {
        "home": {
          "viewName": "Home",
          "viewLevel": 1
        },
        "products": {
          "viewName": "ProductList",
          "viewLevel": 2
        },
        "productDetail": {
          "viewName": "ProductDetail",
          "viewLevel": 2
        },
        "account": {
          "viewName": "Account",
          "viewLevel": 3
        }
      }
    }
  }
}
