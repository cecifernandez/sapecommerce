<mvc:View
    controllerName="ui5.starwarsecommerce.controller.NotFound"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
>
    <IllustratedMessage
        illustrationType="sapIllus-PageNotFound"
        title="{i18n>NotFound}"
        description="{i18n>NotFound.description}"
    >
        <additionalContent>
            <Button
                text="{i18n>NotFound.backToHome}"
                press=".onNavToHome"
            />
        </additionalContent>
    </IllustratedMessage>
</mvc:View>
